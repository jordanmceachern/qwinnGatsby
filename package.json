{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "license": "MIT",
  "scripts": {
    "fix": "eslint --fix:all",
    "build": "gatsby build",
    "build-tw": "npx tailwindcss -i ./src/style-config/directives.css -o ./public/global.css && copyfiles --flat src/style-config/Fonts/*.ttf public",
    "watch-tw": "npx tailwindcss -i ./src/style-config/directives.css -o ./public/global.css --watch",
    "serve": "gatsby serve",
    "dev": "gatsby build && gatsby serve",
    "deploy": "gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@material-tailwind/react": "^2.1.8",
    "@types/react-helmet": "^6.1.11",
    "ansi-html": "0.0.9",
    "ansi-regex": "^6.0.1",
    "cacheable-request": "^10.2.7",
    "copyfiles": "^2.4.1",
    "cross-fetch": "^3.1.5",
    "engine.io": "^6.4.0",
    "eventsource": "^2.0.2",
    "gatsby": "^5.6.1",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-react-helmet": "^6.6.0",
    "gatsby-transformer-remark": "^6.6.0",
    "git": "^0.1.5",
    "glob-parent": "^6.0.2",
    "got": "^12.5.3",
    "http-cache-semantics": "^4.1.1",
    "mime": "^3.0.0",
    "minimatch": "^6.2.0",
    "node-fetch": "^3.3.0",
    "node-forge": "^1.3.1",
    "npm": "^6.14.18",
    "nth-check": "^2.1.1",
    "parse-path": "^7.0.0",
    "parse-url": "^8.1.0",
    "react": "^16.14.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^4.3.1",
    "shell-quote": "^1.8.0",
    "trim": "^1.0.1",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.32.2",
    "gatsby-plugin-postcss": "^6.13.0",
    "gh-pages": "^2.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
