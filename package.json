    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "mime@<1.4.1": ">=1.4.1",
      "cookie@<0.7.0": ">=0.7.0",
      "tmp@<=0.2.3": ">=0.2.4",
      "js-yaml@<3.14.2": ">=3.14.2",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "node-forge@<1.3.2": ">=1.3.2",
      "qs@<6.14.1": ">=6.14.1"
    }
